{
  "name": "react-redux-starter-kit",
  "version": "3.0.0-alpha.2",
  "description": "Get started with React, Redux, and React-Router!",
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "start": "neutrino start --use neutrino-preset-react",
    "build": "neutrino build --use neutrino-preset-react",
    "test": "neutrino test --use neutrino-preset-react neutrino-preset-karma",
    "lint": "eslint src neutrino-custom.js preset-sass.js",
    "deploy": "npm run lint && npm run test && npm run clean && npm run build",
    "clean": "rimraf build",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "neutrino": {
    "use": [
      "neutrino-preset-react",
      "neutrino-preset-karma",
      "neutrino-custom.js",
      "preset-sass.js"
    ],
    "config": {
      "output": {
        "filename": "[name].[hash].js",
        "publicPath": "/"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "bootstrap-sass": "^3.3.7",
    "font-awesome": "^4.7.0",
    "fuzzysearch": "^1.0.3",
    "js-cookie": "^2.1.3",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.8",
    "react": "^15.0.0",
    "react-bootstrap": "^0.31.0",
    "react-dom": "^15.0.0",
    "react-fontawesome": "^1.5.0",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "babel-preset-stage-0": "^6.3.13",
    "codecov": "^2.1.0",
    "css-loader": "^0.28.0",
    "eslint": "^3.16.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^7.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "neutrino": "^5.6.0",
    "neutrino-preset-karma": "^5.6.0",
    "neutrino-preset-react": "^5.6.0",
    "node-sass": "^4.5.0",
    "npm-check-updates": "^2.10.3",
    "postcss-loader": "^2.0.2",
    "react-addons-test-utils": "^15.0.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.2",
    "style-loader": "^0.17.0"
  }
}
